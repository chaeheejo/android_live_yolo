<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.android_live_yolo.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
        <action
            android:id="@+id/action_homeFragment_to_randomFragment"
            app:destination="@id/randomFragment" />

    </fragment>

    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.example.android_live_yolo.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera">

    </fragment>

    <fragment
        android:id="@+id/randomFragment"
        android:name="com.example.android_live_yolo.RandomFragment"
        android:label="fragment_random"
        tools:layout="@layout/fragment_random">

    </fragment>
</navigation>